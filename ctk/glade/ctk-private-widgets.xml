<glade-catalog name="ctkprivate"
	       version="3.10"
	       depends="ctk+"
	       library="ctk-3">
  <init-function>ctk_glade_catalog_init</init-function>

  <glade-widget-classes>
    <!-- base GTK+ private widgets -->
    <glade-widget-class name="GtkPathBar" generic-name="pathbar" title="Path Bar" icon-name="widget-ctk-toolbar"/>
    <glade-widget-class name="GtkColorEditor" generic-name="coloreditor" title="Color Editor" icon-name="widget-ctk-colorselection"/>
    <glade-widget-class name="GtkColorSwatch" generic-name="colorswatch" title="Color Swatch" icon-name="widget-ctk-colorselection"/>
    <glade-widget-class name="GtkColorPlane" generic-name="colorplane" title="Color Plane" icon-name="widget-ctk-colorselection"/>
    <glade-widget-class name="GtkColorScale" generic-name="colorscale" title="Color Scale" icon-name="widget-ctk-colorselection"/>

    <!-- ctkunixprint private widgets -->
    <glade-widget-class name="GtkPrinterOptionWidget" generic-name="printeroptionwidget" title="Printer Option Widget"
			icon-name="widget-ctk-frame">
      <properties>
	<property id="orientation" default="CTK_ORIENTATION_HORIZONTAL"/>
	<property id="size" disabled="True"/>
      </properties>
    </glade-widget-class>
  </glade-widget-classes>
  
  <glade-widget-group name="ctk-private" title="Private GTK+ Classes">
    <glade-widget-class-ref name="GtkPathBar"/>
    <glade-widget-class-ref name="GtkColorEditor"/>
    <glade-widget-class-ref name="GtkColorSwatch"/>
    <glade-widget-class-ref name="GtkColorPlane"/>
    <glade-widget-class-ref name="GtkColorScale"/>
    <glade-widget-class-ref name="GtkPrinterOptionWidget"/>
  </glade-widget-group>
</glade-catalog>
