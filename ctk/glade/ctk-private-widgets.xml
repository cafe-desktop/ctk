<glade-catalog name="ctkprivate"
	       version="3.10"
	       depends="ctk+"
	       library="ctk-3">
  <init-function>ctk_glade_catalog_init</init-function>

  <glade-widget-classes>
    <!-- base CTK+ private widgets -->
    <glade-widget-class name="CtkPathBar" generic-name="pathbar" title="Path Bar" icon-name="widget-ctk-toolbar"/>
    <glade-widget-class name="CtkColorEditor" generic-name="coloreditor" title="Color Editor" icon-name="widget-ctk-colorselection"/>
    <glade-widget-class name="CtkColorSwatch" generic-name="colorswatch" title="Color Swatch" icon-name="widget-ctk-colorselection"/>
    <glade-widget-class name="CtkColorPlane" generic-name="colorplane" title="Color Plane" icon-name="widget-ctk-colorselection"/>
    <glade-widget-class name="CtkColorScale" generic-name="colorscale" title="Color Scale" icon-name="widget-ctk-colorselection"/>

    <!-- ctkunixprint private widgets -->
    <glade-widget-class name="CtkPrinterOptionWidget" generic-name="printeroptionwidget" title="Printer Option Widget"
			icon-name="widget-ctk-frame">
      <properties>
	<property id="orientation" default="CTK_ORIENTATION_HORIZONTAL"/>
	<property id="size" disabled="True"/>
      </properties>
    </glade-widget-class>
  </glade-widget-classes>
  
  <glade-widget-group name="ctk-private" title="Private CTK+ Classes">
    <glade-widget-class-ref name="CtkPathBar"/>
    <glade-widget-class-ref name="CtkColorEditor"/>
    <glade-widget-class-ref name="CtkColorSwatch"/>
    <glade-widget-class-ref name="CtkColorPlane"/>
    <glade-widget-class-ref name="CtkColorScale"/>
    <glade-widget-class-ref name="CtkPrinterOptionWidget"/>
  </glade-widget-group>
</glade-catalog>
