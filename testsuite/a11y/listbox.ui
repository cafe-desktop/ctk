<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires ctk+ 3.0 -->
  <object class="CtkWindow" id="window1">
    <property name="can_focus">False</property>
    <property name="type">popup</property>
    <child>
      <object class="CtkBox" id="box1">
        <property name="visible">True</property>
        <child>
          <object class="CtkListBox" id="listbox1">
            <property name="visible">True</property>
            <property name="selection-mode">none</property>
            <child>
              <object class="CtkLabel" id="row1">
                <property name="visible">True</property>
                <property name="label">Row One</property>
              </object>
            </child>
            <child>
              <object class="CtkLabel" id="row2">
                <property name="visible">True</property>
                <property name="label">Row Two</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="CtkListBox" id="listbox2">
            <property name="visible">True</property>
            <child>
              <object class="CtkLabel" id="row3">
                <property name="visible">True</property>
                <property name="label">Row Tree</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
