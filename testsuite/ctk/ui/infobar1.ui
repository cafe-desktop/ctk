<!-- test all valid infobar markup -->
<interface>
  <object class="CtkInfoBar">
    <child>
      <object class="CtkButton" id="button_cancel"/>
    </child>
    <child>
      <object class="CtkButton" id="button_ok">
        <property name="can-default">True</property>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-3">button_cancel</action-widget>
      <action-widget response="ok">button_ok</action-widget>
    </action-widgets>
  </object>
</interface>
