copy ..\cdk-3.0.pc $(CopyDir)\lib\pkgconfig&#x0D;&#x0A;
copy ..\cdk-3.0.pc $(CopyDir)\lib\pkgconfig\cdk-win32-3.0.pc&#x0D;&#x0A;
copy &quot;..\ctk+-3.0.pc&quot; $(CopyDir)\lib\pkgconfig&#x0D;&#x0A;
