copy $(ConfigurationName)\$(PlatformName)\bin\$(CtkDllPrefix)cdk-3$(CtkDllSuffix).pdb $(CopyDir)\bin&#x0D;&#x0A;
copy $(ConfigurationName)\$(PlatformName)\bin\cdk-$(ApiVersion).lib $(CopyDir)\lib&#x0D;&#x0A;

