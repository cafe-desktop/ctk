msgid "Opcode"
msgstr "Opcode"

#: cdk/x11/cdkdevicemanager-xi2.c:116
msgid "Opcode for XInput2 requests"
msgstr "Opcode airson iarrtasan XInput2"

#: cdk/x11/cdkdevicemanager-xi2.c:122
msgid "Major"
msgstr "Mòr-thionndadh"

#: cdk/x11/cdkdevicemanager-xi2.c:123
msgid "Major version number"
msgstr "Àireamh a’ mhòr-thionndaidh"

#: cdk/x11/cdkdevicemanager-xi2.c:129
msgid "Minor"
msgstr "Mion-tionndadh"

#: cdk/x11/cdkdevicemanager-xi2.c:130
msgid "Minor version number"
msgstr "Àireamh a’ mhion-tionndaidh"

#: cdk/x11/cdkdevice-xi2.c:143
msgid "Device ID"
msgstr "ID an uidheim"

#: cdk/x11/cdkdevice-xi2.c:144
msgid "Device identifier"
msgstr "Aithnichear an uidheim"

