#: ctk/ctksettings.c:932
msgid "Keynav Wrap Around"
msgstr "Körkörös billentyűnavigáció"

#: ctk/ctksettings.c:933
msgid "Whether to wrap around when keyboard-navigating widgets"
msgstr ""
"A felületi elemeken körbe lehet-e lépkedni a billentyűnavigáció használatával"

#: ctk/ctksettings.c:953
msgid "Error Bell"
msgstr "Hibacsengő"

#: ctk/ctksettings.c:954
msgid "When TRUE, keyboard navigation and other errors will cause a beep"
msgstr ""
"Ha IGAZ, akkor a billentyűnavigációs és egyéb hibák hangjelzést okoznak"

#: ctk/ctksettings.c:973
msgid "Color Hash"
msgstr "Szín hash"

#: ctk/ctksettings.c:974
msgid "A hash table representation of the color scheme."
msgstr "A színséma hashtábla-reprezentációja"

#: ctk/ctksettings.c:989
msgid "Default file chooser backend"
msgstr "A fájlválasztó alapértelmezett háttérprogramja"

#: ctk/ctksettings.c:990
msgid "Name of the CtkFileChooser backend to use by default"
msgstr ""
"Az alapértelmezés szerint használandó CtkFileChooser háttérprogram neve"

#: ctk/ctksettings.c:1007
msgid "Default print backend"
msgstr "Alapértelmezett nyomtatási háttérprogram"

#: ctk/ctksettings.c:1008
msgid "List of the CtkPrintBackend backends to use by default"
msgstr ""
"Az alapértelmezés szerint használandó CtkPrintBackend háttérprogramok listája"

#: ctk/ctksettings.c:1031
msgid "Default command to run when displaying a print preview"
msgstr ""
"Nyomtatási előnézet megjelenítésekor futtatandó alapértelmezett parancs"

#: ctk/ctksettings.c:1032
msgid "Command to run when displaying a print preview"
msgstr "Nyomtatási előnézet megjelenítésekor futtatandó parancs"

#: ctk/ctksettings.c:1051
msgid "Enable Mnemonics"
msgstr "Gyorsbillentyűk bekapcsolása"

#: ctk/ctksettings.c:1052
msgid "Whether labels should have mnemonics"
msgstr "A címkék rendelkezzenek-e hívóbetűkkel"

#: ctk/ctksettings.c:1068
msgid "Enable Accelerators"
msgstr "Gyorsbillentyűk engedélyezése"

#: ctk/ctksettings.c:1069
msgid "Whether menu items should have accelerators"
msgstr "A menüelemek rendelkezzenek-e gyorsbillentyűkkel"

#: ctk/ctksettings.c:1088
msgid "Recent Files Limit"
msgstr "Legutóbbi fájlok korlátja"

#: ctk/ctksettings.c:1089
msgid "Number of recently used files"
msgstr "A legutóbb használt fájlok száma"

#: ctk/ctksettings.c:1109
msgid "Default IM module"
msgstr "Alapértelmezett beviteli modul"

#: ctk/ctksettings.c:1110
msgid "Which IM module should be used by default"
msgstr "Az alapértelmezetten használandó beviteli modul"

#: ctk/ctksettings.c:1128
msgid "Recent Files Max Age"
msgstr "Legutóbbi fájlok maximális kora"

#: ctk/ctksettings.c:1129
msgid "Maximum age of recently used files, in days"
msgstr "A legutóbb használt fájlok maximális kora napokban"

#: ctk/ctksettings.c:1138
msgid "Fontconfig configuration timestamp"
msgstr "Fontconfig konfigurációs időbélyeg"

#: ctk/ctksettings.c:1139
msgid "Timestamp of current fontconfig configuration"
msgstr "A jelenlegi Fontconfig konfiguráció időbélyege"

#: ctk/ctksettings.c:1161
msgid "Sound Theme Name"
msgstr "Hangtéma neve"

#: ctk/ctksettings.c:1162
msgid "XDG sound theme name"
msgstr "XDG hangtéma neve"

#. Translators: this means sounds that are played as feedback to user input
#: ctk/ctksettings.c:1184
msgid "Audible Input Feedback"
msgstr "Hallható bemenet-visszajelzés"

#: ctk/ctksettings.c:1185
