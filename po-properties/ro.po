msgid "A unique name for the action."
msgstr "Un nume unic pentru acțiune."

#: ctk/ctkaction.c:264 ctk/ctkbutton.c:281 ctk/ctkexpander.c:308
#: ctk/ctkframe.c:231 ctk/ctklabel.c:805 ctk/ctkmenuitem.c:789
#: ctk/ctktoolbutton.c:243 ctk/ctktoolitemgroup.c:1639
msgid "Label"
msgstr "Etichetă"

#: ctk/ctkaction.c:265
msgid "The label used for menu items and buttons that activate this action."
msgstr ""
"Etichetă utilizată pentru elementele din meniu și butoanele ce activează "
"această acțiune."

#: ctk/ctkaction.c:284
msgid "Short label"
msgstr "Etichetă scurtă"

#: ctk/ctkaction.c:285
msgid "A shorter label that may be used on toolbar buttons."
msgstr "O etichetă mai scurtă de utilizat pentru butoanele din bara cu unelte."

#: ctk/ctkaction.c:300
msgid "Tooltip"
msgstr "Indiciu"

#: ctk/ctkaction.c:301
msgid "A tooltip for this action."
msgstr "Un indiciu pentru această acțiune."

#: ctk/ctkaction.c:319
msgid "Stock Icon"
msgstr "Iconiță din stoc"

#: ctk/ctkaction.c:320
msgid "The stock icon displayed in widgets representing this action."
msgstr "Iconița din stoc de afișat într-un widget asociat acelei acțiuni."

#: ctk/ctkaction.c:342 ctk/deprecated/ctkstatusicon.c:279
msgid "GIcon"
msgstr "GIcon"

#: ctk/ctkaction.c:343 ctk/deprecated/ctkstatusicon.c:280
#: ctk/ctkcellrendererpixbuf.c:282 ctk/ctkimage.c:360
msgid "The GIcon being displayed"
msgstr "Elementul GIcon afișat"

